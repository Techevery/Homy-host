{
  "name": "homeyhost",
  "version": "1.0.0",
  "description": "Backend service for Homeyhost apartment booking service",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run prisma:studio\"",
    "status": "npx prisma migrate status",
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset --force && ts-node --transpile-only prisma/seed.ts",
    "prisma:seed": "ts-node --transpile-only prisma/seed.ts",
    "prisma:generate": "npx prisma generate",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Techevery/homeyhost.git"
  },
  "keywords": [
    "studio",
    "apartment",
    "bookings"
  ],
  "author": "Adeolu Joseph",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Techevery/homeyhost/issues"
  },
  "homepage": "https://github.com/Techevery/homeyhost#readme",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@supabase/supabase-js": "^2.47.10",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.5.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "googleapis": "^144.0.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.12.0",
    "nanoid": "^5.1.2",
    "pg": "^8.13.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "postgres": "^3.4.5",
    "prisma": "^5.22.0",
    "slugify": "^1.6.6",
    "validator": "^13.15.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/mysql": "^2.15.26",
    "@types/node": "^22.7.5",
    "@types/validator": "^13.15.0",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "base64-arraybuffer": "^1.0.2",
    "concurrently": "^9.0.1",
    "eslint": "^9.18.0",
    "globals": "^15.14.0",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.20.0"
  }
}
